{
  "name": "vuerd-vscode",
  "version": "0.6.5",
  "displayName": "ERD Editor",
  "description": "ERD Editor vscode extension",
  "main": "./out/extension.js",
  "private": true,
  "icon": "img/vuerd.png",
  "publisher": "dineug",
  "categories": [
    "Other"
  ],
  "keywords": [
    "vuerd",
    "ERD",
    "Editor",
    "vscode",
    "db",
    "database",
    "diagram"
  ],
  "homepage": "https://github.com/vuerd/vuerd-vscode",
  "bugs": {
    "url": "https://github.com/vuerd/vuerd-vscode/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vuerd/vuerd-vscode.git"
  },
  "author": "dineug <dineug2@gmail.com>",
  "license": "MIT",
  "engines": {
    "vscode": "^1.47.0"
  },
  "activationEvents": [
    "onCommand:vuerd.webview",
    "onWebviewPanel:vuerd.webview",
    "onCustomEditor:vuerd.editor"
  ],
  "contributes": {
    "customEditors": [
      {
        "viewType": "vuerd.editor",
        "displayName": "vuerd",
        "selector": [
          {
            "filenamePattern": "*.vuerd"
          }
        ]
      }
    ],
    "languages": [
      {
        "id": "vuerd",
        "extensions": [
          ".vuerd"
        ],
        "aliases": [
          "vuerd"
        ]
      }
    ],
    "commands": [
      {
        "command": "vuerd.webview",
        "title": "Open ERD Editor",
        "category": "vuerd",
        "icon": {
          "light": "./img/vuerd.png",
          "dark": "./img/vuerd.png"
        }
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "vuerd.webview",
          "when": "resourceFilename =~ /.*\\.(vuerd.json)/",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "command": "vuerd.webview",
          "when": "resourceFilename =~ /.*\\.(vuerd.json)/",
          "group": "navigation"
        }
      ],
      "editor/title/context": [
        {
          "command": "vuerd.webview",
          "when": "resourceFilename =~ /.*\\.(vuerd.json)/",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "vuerd.webview",
          "when": "resourceFilename =~ /.*\\.(vuerd.json)/",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "rollup -c",
    "lint": "eslint . --ext .ts"
  },
  "devDependencies": {
    "@dineug/vscode-google-analytics": "^0.1.2",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/node": "^14.6.1",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "macaddress": "^0.5.1",
    "prettier": "^2.0.5",
    "rollup": "^2.26.6",
    "rollup-plugin-eslint": "^7.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {}
}
