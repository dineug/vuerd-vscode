{
  "name": "vuerd-vscode",
  "displayName": "ERD Editor",
  "description": "ERD Editor vscode extension",
  "version": "0.5.5",
  "icon": "img/vuerd.png",
  "publisher": "dineug",
  "homepage": "https://github.com/vuerd/vuerd-vscode",
  "bugs": {
    "url": "https://github.com/vuerd/vuerd-vscode/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vuerd/vuerd-vscode.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "vuerd",
    "ERD",
    "Editor",
    "vscode",
    "db",
    "database"
  ],
  "activationEvents": [
    "onCommand:vuerd.open",
    "onWebviewPanel:vuerd"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "vuerd",
        "extensions": [
          ".vuerd.json"
        ],
        "aliases": [
          "vuerd"
        ]
      }
    ],
    "commands": [
      {
        "command": "vuerd.open",
        "title": "Open ERD Editor",
        "category": "vuerd",
        "icon": {
          "light": "./img/vuerd.png",
          "dark": "./img/vuerd.png"
        }
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "vuerd.open",
          "when": "resourceLangId == vuerd",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "command": "vuerd.open",
          "when": "editorLangId == vuerd",
          "group": "navigation"
        }
      ],
      "editor/title/context": [
        {
          "command": "vuerd.open",
          "when": "resourceLangId == vuerd",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "vuerd.open",
          "when": "editorLangId == vuerd",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/node": "^14.0.4",
    "@types/vscode": "^1.45.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "electron-google-analytics": "^0.3.0"
  }
}
